{
  "name": "pryley/castor",
  "license": "MIT",
  "description": "Parent theme for Dioscuri",
  "homepage": "https://github.com/pryley/castor",
  "keywords": ["wordpress", "dioscuri", "castor", "theme", "pryley"],
  "authors": [
    {
      "name": "Paul Ryley",
      "email": "paul@geminilabs.io"
    }
  ],
  "support": {
    "issues": "https://github.com/pryley/castor/issues",
    "source": "https://github.com/pryley/castor"
  },
  "archive": {
    "exclude": []
  },
  "require": {
    "php": ">=7.1.0",
